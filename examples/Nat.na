<m: {Nat : *
    ;zero : Nat
    ;succ : Nat -> Nat
--     ;plus : Nat -> Nat -> Nat
}> 
split m into Nat,zero,succ in
(Nat = { t : [zer,suc]; 
         v : case t of {
                zer = [tt],
                suc = Nat} : *}
,zero = (t = 'zer, v = 'tt)
,succ = \(n : ?N) -> (t = 'suc, v = n)
